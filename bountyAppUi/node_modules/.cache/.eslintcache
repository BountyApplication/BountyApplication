 {\r\n    balance: PropTypes.number.isRequired,\r\n    sum: PropTypes.number.isRequired,\r\n};\r\n\r\nBalanceInfos.defaultProps = {\r\n    balance: null,\r\n    sum: null,\r\n};\r\n\r\nexport default function BalanceInfos({balance, sum}) {\r\n    return(\r\n        <div className=\"rubric\">\r\n            <div className=\"title\">{\"Kontodaten\"}</div>\r\n            <div className=\"wrapper\">\r\n            {`Stand: ${balance?`${balance.toFixed(2)}€`:null}`}<br/>\r\n            {`Summe: ${sum?`${sum.toFixed(2)}€`:null}`} <br />\r\n            {`Neu: ${balance&&sum?`${(balance-sum).toFixed(2)}€`:null}`}\r\n            </div>\r\n        </div>\r\n    );\r\n}","C:\\Users\\Isaja\\my-app\\bountyAppUi\\src\\bounty\\generalUi\\BalanceCorrection.js",["202"],[],"import NumberInput from '../util/NumberInput';\r\nimport PropTypes from 'prop-types';\r\n\r\nBalanceCorrection.propTypes = {\r\n    plus: PropTypes.number,\r\n    minus: PropTypes.number,\r\n\r\n    setPlus: PropTypes.func.isRequired,\r\n    setMinus: PropTypes.func.isRequired,\r\n};\r\n\r\nBalanceCorrection.defaultProps = {\r\n    plus: null,\r\n    minus: null,\r\n\r\n    setPlus: (v) => {},\r\n    setMinus: (v) => {},\r\n};\r\n\r\nexport default function BalanceCorrection({plus, minus, setPlus, setMinus}) {\r\n    return(\r\n        <div className='rubric'>\r\n            <div className='title'>{\"Korrekturbuchung\"}</div>\r\n            <div className='wrapper'>{\"Plus-Korrektur: \"} <NumberInput value={plus} setValue={setPlus} /></div>\r\n            <div className='wrapper'>{\"Minus-Korrektur: \"} <NumberInput value={minus} setValue={setMinus} /></div>\r\n        </div>\r\n    );\r\n}","C:\\Users\\Isaja\\my-app\\bountyAppUi\\src\\bounty\\generalUi\\ProductDisplay.js",["203","204","205","206"],[],"C:\\Users\\Isaja\\my-app\\bountyAppUi\\src\\bounty\\generalUi\\ChashPayment.js",["207"],[],"import NumberInput from \"../util/NumberInput\";\r\nimport PropTypes from \"prop-types\";\r\n\r\nCashPayment.propTypes = {\